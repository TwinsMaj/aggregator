{
	"name": "ts-boilerplate",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "rimraf dist && swc ./src -d dist/src && npm run build:knexfile",
		"build:knexfile": "swc ./knexfile.ts -o dist/knexfile.js && rimraf dist/knexfile.js.map",
		"start": "node dist/src/index.js",
		"start:dev": "npm run build && npm run start",
		"db:migrate": "knex migrate:latest",
		"db:seed": "knex seed:run",
		"copy-env": "cp .env.example .env"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/lodash": "^4.14.182",
		"chalk": "^4.1.2",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"http-status-codes": "^2.2.0",
		"joi": "^17.6.0",
		"knex": "^2.1.0",
		"lodash": "^4.17.21",
		"pg": "^8.7.3",
		"typescript": "^4.6.4"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.187",
		"@tsconfig/node16": "^1.0.2",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.5.1",
		"@types/node": "^17.0.35",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
